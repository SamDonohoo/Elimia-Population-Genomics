# Converting Loci to Fasta Files follwed by Concatation into a Supermatrix using Modified ElimiaM Dataset

python3 Convert_Stacks_Fasta_to_Loci.py --fast Ebell_m5M2n2_Populations_5_75_025_05_Corrected_Multi/populations.samples.fa --outdir Fasta_to_Loci/ -a consensus

perl /usr/local/genome/bin/FASconCAT-G_v1.04.pl -s

# Phylogenetic Inference in IQTREE

iqtree -s E_no19_wOutgroup_supermatrix.fas -spp E_no19_wOutgroup_partition.txt -m MFP+MERGE -mset GTR -merit BIC -rcluster 10 -rcluster-max 1000 -safe -cptime 14400 -bb 1000 --runs 10

# Species Tree Inference in SNAPP - Reduced ElimiaS Dataset (Five Individuals per Species)

~/Programs/beast/bin/beast -threads 24 -beagle_SSE -beagle_scaling dynamic bcNWR_NoOut_populations.snps.reduced_5per_2.5Mgens.xml
